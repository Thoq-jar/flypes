{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "outDir": "./static/js",
    "rootDir": "./static/js",
    "jsx": "react"
  },
  "include": ["static/js/**/*"],
  "exclude": ["node_modules"]
}
